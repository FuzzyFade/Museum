/* pages/databaseGuide/databaseGuide.wxss */

.contain {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    overflow: hidden;
}

.content {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.background {
    width: 100%;
    height: 100%;
}

.head {
    margin-top: 80px;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: flex-end;
    width: 100%;
}

.tit {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 218px;
    height: 58px;
    color: #3B5142;
    font-size: 20px;
}

.button {
    height: 36px;
    width: 36px;
    margin-left: 22px;
}

.border {
    position: absolute;
    width: 218px;
    height: 58px;
}